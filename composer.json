{
    "name": "dusty_meg/seat-extra-sde",
    "description": "A SEAT plugin to allow the inclusion of additional SDE tables into the Seat database.",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Dusty Meg",
            "email": "dust-mite@hotmail.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dusty_Meg\\Seat\\SeatExtraSDE\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "laravel/framework": "^6.2",
        "eveseat/web": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "orchestra/database": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dusty_Meg\\Seat\\SeatExtraSDE\\ExtraSDEServiceProvider"
            ]
        }
    }
}
